    "jest-expo": "54.0.12",
    "lint-staged": "16.2.3",
    "typescript": "5.9.2"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
